if(gillion.notice!==false){
if(gillion.notice=='enable2'){
$('.sh-page-notice').show();
$(".sh-page-notice-button").on('click', function(){
$('.sh-page-notice').hide();
return false;
});
}else{
if('set'!==$.cookie('sh-notice')){
$('.sh-page-notice').show();
$(".sh-page-notice-button").on('click', function(){
$.cookie('sh-notice', 'set', { expires: 356, path: '/' });
$('.sh-page-notice').hide();
return false;
});
}}
}
