var pix=pix||{};pix.js_error_type=pix.js_error_type||"other",pix.js_logger=function(e){/pixnet/.test(window.location.hostname)&&("undefined"!=typeof pix.js_error_alert&&pix.js_error_alert&&alert(e),"undefined"!=typeof console&&"undefined"!=typeof console.log&&console.log(e),(new Image).src="//error.pixplug.in/"+pix.js_error_type+".gif?message="+encodeURIComponent(e)+"&from="+encodeURIComponent(document.location))},pix.window_error=function(e,o,r){"object"==typeof e&&(o=e.target.src,e=e.type,r=0),pix.js_logger("window.onerror: "+e+" "+o+" "+r)},"undefined"!=typeof window.addEventListener?window.onerror=pix.window_error:"undefined"!=typeof window.attachEvent&&window.attachEvent("error",pix.window_error),jQuery(function(){jQuery("<div>").ajaxError(function(e,o,r,n){pix.js_logger("AjaxError:"+r.type+" "+r.url+" "+o.status+" ("+o.statusText+"):"+n)})}),"undefined"!=typeof console&&"undefined"!=typeof console.error&&(console.error=function(){for(var e=[],o=0;o<arguments.length;o++)e.push(" "+arguments[o]);pix.js_logger("console.error: "+e.join(" "))});